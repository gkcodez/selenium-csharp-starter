<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
	</startup>
	<appSettings>
		<!-- Default : Chrome | Values : Chrome | Firefox | IE | Safari -->
		<add key="browser" value="Chrome" />
		<add key="appname" value="New_Tours" />
		<add key="url" value="http://www.newtours.demoaut.com" />
		<!--Default : 30 seconds-->
		<add key="timeoutshort" value="10" />
		<!--Default : 60 seconds-->
		<add key="timeoutmedium" value="30" />
		<!--Default : 120 seconds-->
		<add key="timeoutlong" value="60" />
		<!-- Default : true | Values : true | false -->
		<add key="logenabled" value="false" />
		<!-- Default : true | Values : true | false -->
		<add key="screenshotenabled" value="true" />
		<!-- Default : true | Values : true | false -->
		<add key="reportenabled" value="true" />
		<!--Default : Resources-->
		<add key="driverpath" value="Resources" />
		<!--Default : TestOutput\Logs-->
		<add key="logpath" value="" />
		<!--Default : TestOutput\Screenshots-->
		<add key="screenshotpath" value="" />
		<!--Default : TestOutput\Reports-->
		<add key="reportpath" value="" />
		<!-- Default : png | Values : jpg | png | gif | bmp -->
		<add key="screenshotformat" value="jpg" />
		<!-- Default : txt | Values : txt | csv-->
		<add key="logformat" value="csv" />
	</appSettings>
	<connectionStrings>
		<add name="Automation" connectionString="Data Source=(LocalDb)\MSSQLLocalDB;Initial Catalog=Automation;Integrated Security=True" />
	</connectionStrings>
</configuration>